{
  "title"    : {{ post.title | jsonify }},
  "category" : {{ post.category | jsonify }},
  "url"     : {{ post.url  | jsonify}},
  "image"     : {{ post.image  | jsonify}},
  "date"     : {
    "day"   : "{{ post.date | date: "%d" }}",
    "month" : "{{ post.date | date: "%B" }}",
    "year"  : "{{ post.date | date: "%Y" }}"
  }
}
